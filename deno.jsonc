{
  "importMap": "./import_map.json",
  // this is tasks
  "tasks": {
    "deno": "deno run --import-map ./import_map.json -A --unstable",
    "dev": "deno run --import-map ./import_map.json -A --no-check --unstable x.ts",
    "test": "deno test --import-map ./import_map.json --allow-env --allow-read --allow-run --unstable",
    "release": "x release"
  }
}
