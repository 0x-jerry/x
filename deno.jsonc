{
  "importMap": "./import_map.json",
  // this is tasks
  "tasks": {
    "deno": "deno run -A",
    "dev": "deno run -A --no-check x.ts",
    "test": "deno test --allow-env --allow-read --allow-run",
    // example: xr install x.ts
    "install": "deno install -r -Af --import-map ./import_map.json",
    "release": "x release"
  }
}
